		<script>
			document.addEventListener('DOMContentLoaded', function () {
				// Function to get URL parameters
				function getParameterByName(name) {
					const url = window.location.href
					name = name.replace(/[\[\]]/g, '\\$&')
                const regex = new RegExp('[?&]' + name + '(=([^&#]*)|&|#|$)')
					const results = regex.exec(url)

					if (!results) return null
					if (!results[2]) return ''

					return decodeURIComponent(results[2].replace(/\+/g, ' '))
				}

				// Get the gclid value from URL
				const gclidValue = getParameterByName('gclid')

				// Update the hidden input if gclid exists in URL
				if (gclidValue) {
					console.log('gclid: ', gclidValue)
                const gclidInput = document.querySelector('input[name="gclid"]')
					if (gclidInput) {
						gclidInput.value = gclidValue
					}
				}
			})
		</script>
